class Solution {
    public long flowerGame(int n, int m) {
        //alice will win only when one of the lane is even and the other one is odd
       long oddN = (n + 1) / 2;
        long evenN = n / 2;
        long oddM = (m + 1) / 2;
        long evenM = m / 2;

        return oddN * evenM + evenN * oddM;
    }
}
