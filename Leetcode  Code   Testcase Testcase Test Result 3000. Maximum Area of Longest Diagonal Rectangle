class Solution {
    public int areaOfMaxDiagonal(int[][] dimensions) {
        int area=0;
        int maxDiagonalsq = -1;
        for(int i=0;i<dimensions.length;i++){
            int m = dimensions[i][0]*dimensions[i][0];
            int n = dimensions[i][1]*dimensions[i][1];
            int diagonalSq = m+n;

            if(diagonalSq > maxDiagonalsq){
               area = dimensions[i][0]*dimensions[i][1];
               maxDiagonalsq = diagonalSq;
            }
            else if(diagonalSq == maxDiagonalsq ){
                area = Math.max(area,dimensions[i][0]*dimensions[i][1]);
            }
        }

        return area;
    }
}
